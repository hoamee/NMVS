@model NMVS.Models.DbModels.SystemMessage

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Create</h1>

<h4>SystemMessage</h4>
<hr />
<ul>
    <li>
        Movement: 0~99
    </li>
    <li>
        Configuration Data: 100~199
    </li>
    <li>
        System: 200~299
    </li>
    <li>
        Excel Import: 300~399
    </li>
    <li>
        Usage: 400~499
    </li>
</ul>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MsgNo" class="control-label">Msg No.</label>
                <input asp-for="MsgNo" class="form-control" />
                <span asp-validation-for="MsgNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Message" class="control-label"></label>
                <input asp-for="Message" class="form-control" />
                <span asp-validation-for="Message" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

